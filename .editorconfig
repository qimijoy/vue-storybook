root = true

[*]
charset = utf-8
indent_style = tab
indent_size = tab
insert_final_newline = true
trim_trailing_whitespace = true
end_of_line = lf

[*.js]
max_line_length = 120

[{*.json,*.yml}]
indent_style = space
indent_size = 2
insert_final_newline = false

[*.svg]
insert_final_newline = false

[*.md]
trim_trailing_whitespace = false

[*.bat]
end_of_line = crlf

# Files without extensions
[*.*]
